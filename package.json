    "fs-extra": "0.30.0",
    "lodash": "4.15.0",
    "melanke-watchjs": "1.3.1",
    "mime": "1.3.4",
    "moment": "2.14.1",
    "mongodb": "2.2.24",
    "nedb": "1.8.0",
    "source-map-support": "0.4.15",
    "stylus": "0.54.5",
    "xlsx.js": "2.3.1"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "coffee-script": "1.12.6",
    "electron": "1.6.11",
    "electron-builder": "19.13.0",
    "gulp": "3.9.1",
    "gulp-coffee": "2.3.4",
