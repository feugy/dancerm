    "vendor/angular-elastic.js": "https://raw.githubusercontent.com/monospaced/angular-elastic/master/elastic.js",
    "src/style/css/bootstrap.css": "http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css",
